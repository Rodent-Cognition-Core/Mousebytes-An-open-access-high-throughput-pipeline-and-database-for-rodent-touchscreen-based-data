                <value>30</value>
            </Condition>
            <Condition>
                <name>Max_Schedule_Time</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>Analysis_Name</name>
                <value>5C_Touch Must Touch</value>
            </Condition>
            <Condition>
                <name>End Summary - Condition</name>
                <value>&gt;0</value>
            </Condition>
            <Condition>
                <name>End Summary - Corrects</name>
                <value>&gt;0</value>
            </Condition>
        </Schedule>

        <Schedule>
