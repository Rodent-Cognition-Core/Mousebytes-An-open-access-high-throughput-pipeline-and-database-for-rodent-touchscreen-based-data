<qc>
    <Database name="LON5C"> 
        <!-- NOTE: Multiple names should be separated by 3 "|"s. -->
        <!-- tag Schedule must be followed by name tag, then list of Conditions. 
             Only 2 related conditions are allowed. 
             if v1 < given value, v2 must equal to the given value. Or
             if v2 < given value, v1 must equal to the given value.
        -->
        <Schedule>
            <name>.*Initial[_\W]Touch</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - No. images</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>.*Must[_\W]Touch</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Corrects</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>.*Must[_\W]Initiate</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Corrects</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>.*Punish[_\W]Incorrect</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Trials Completed</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>5CSRTT_(8s|8000ms|4s|4000ms|2s|2000ms|1.5s|1500ms|1s|1000ms|0.8s|800ms|0.6s|600ms|0.4s|400ms|0.2s|200ms)_Var1</name>
            <Condition>
                <name>Threshold - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>Threshold - Trials</name>
                <value>50</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>5CSRTT_(2s|2000ms)_INTRAPROBE_Var1</name>
            <Condition>
                <name>Threshold - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>Threshold - Trials</name>
                <value>50</value>
            </Condition>
        </Schedule>

    </Database>
</qc>
