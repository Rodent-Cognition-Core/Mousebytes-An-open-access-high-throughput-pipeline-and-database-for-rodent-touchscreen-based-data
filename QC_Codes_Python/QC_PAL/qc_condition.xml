<qc>
    <Database name="LON5C"> 
        <!-- NOTE: Multiple names should be separated by 3 "|"s. -->
        <!-- tag Schedule must be followed by name tag, then list of Conditions. 
             Only 2 related conditions are allowed. 
             if v1 < given value, v2 must equal to the given value. Or
             if v2 < given value, v1 must equal to the given value.
        -->
        <Schedule>
            <name>.*Initial[_\W]Touch</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - No. images</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>.*Must[_\W]Touch</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Corrects</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>.*Must[_\W]Initiate</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Corrects</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>.*Punish[_\W]Incorrect</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Trials Completed</name>
                <value>30</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>acquisition</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Trials Completed</name>
                <value>36</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>Mouse[_\W][ds]PAL (?!acquisition|RETENTION)</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Trials Completed</name>
                <value>36</value>
            </Condition>
        </Schedule>

        <Schedule>
            <name>Mouse[_\W][ds]PAL RETENTION</name>
            <Condition>
                <name>End Summary - Condition</name>
                <value>3600</value>
            </Condition>
            <Condition>
                <name>End Summary - Trials Completed</name>
                <value>36</value>
            </Condition>
        </Schedule>

    </Database>
</qc>
