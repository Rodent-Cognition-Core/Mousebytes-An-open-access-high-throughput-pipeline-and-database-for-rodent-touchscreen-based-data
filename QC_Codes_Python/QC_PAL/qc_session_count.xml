<!-- This is to check each animal ID shoud have a certain number of sessions for each schedule -->
<qc>
    <Database name="LONPAL"> 
        <Schedule>
            <name>.*Habituation[_\W]1</name>
            <keyword>Habituation 1</keyword>
            <sessionNumber>==1</sessionNumber>
        </Schedule>
        
        <Schedule>
            <name>.*Habituation[_\W]2</name>
            <keyword>Habituation 2</keyword>
            <sessionNumber>==3</sessionNumber>
        </Schedule>
        
        <Schedule>
            <name>.*Initial[_\W]Touch</name>
            <keyword>Initial Touch</keyword>
            <sessionNumber>&gt;0</sessionNumber>
        </Schedule>

        <Schedule>
            <name>.*Must[_\W]Touch</name>
            <keyword>Must Touch</keyword>
            <sessionNumber>&gt;0</sessionNumber>
        </Schedule>

        <Schedule>
            <name>.*Must[_\W]Initiate</name>
            <keyword>Must Initiate</keyword>
            <sessionNumber>&gt;0</sessionNumber>
        </Schedule>

        <Schedule>
            <name>.*Punish[_\W]Incorrect</name>
            <keyword>Punish Incorrect</keyword>
            <sessionNumber>&gt;1</sessionNumber>
        </Schedule>

        <Schedule>
            <name>acquisition</name>
            <keyword>acquisition</keyword>
            <sessionNumber>&gt;1</sessionNumber>
        </Schedule>

        <Schedule>
            <name>Mouse[_\W]dPAL (?!acquisition|RETENTION)</name>
            <keyword>Mouse dPAL</keyword>
            <sessionNumber>&gt;44</sessionNumber>
        </Schedule>

        <Schedule>
            <name>Mouse[_\W]sPAL (?!acquisition|RETENTION)</name>
            <keyword>Mouse sPAL</keyword>
            <sessionNumber>&gt;44</sessionNumber>
        </Schedule>

        <Schedule>
            <name>Mouse[_\W]dPAL[_\W]RETENTION</name>
            <keyword>Mouse dPAL RETENTION</keyword>
            <sessionNumber>&gt;4</sessionNumber>
        </Schedule>

        <Schedule>
            <name>Mouse[_\W]sPAL[_\W]RETENTION</name>
            <keyword>Mouse sPAL RETENTION</keyword>
            <sessionNumber>&gt;4</sessionNumber>
        </Schedule>

    </Database>
</qc>
