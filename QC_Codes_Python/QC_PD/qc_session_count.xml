<!-- This is to check each animal ID shoud have a certain number of sessions for each schedule -->
<qc>

    <Database name="LONPD">
        <Schedule>
            <name>.*Baseline</name>
            <keyword>Baseline</keyword>
            <sessionNumber>==2</sessionNumber>
        </Schedule>

        <Schedule>
            <name>.*[_\W]Reversal[_\W]</name>
            <keyword>Reversal</keyword>
            <sessionNumber>==10</sessionNumber>
        </Schedule>

        <Schedule>
            <name>.*RetentionReversal</name>
            <keyword>RetentionReversal</keyword>
            <sessionNumber>==2</sessionNumber>
        </Schedule>
    </Database>
</qc>
